<% content_for :javascript_includes do %>
    <%= javascript_tag do %>
        window.songs_json = <%= raw @songs.to_json %>;
    <% end %>
    <%= javascript_include_tag "application" %>
    <%= javascript_include_tag "player" %>
<% end %>

<% content_for :css_includes do %>
<%= stylesheet_link_tag    "player", media: "all", "data-turbolinks-track" => true %>
<% end %>

<%= render "aside_menu" %>

<%= render "app_main" %>